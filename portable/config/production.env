# Configuration Production - Sprint 1.8
PORTABLE_MODE=true
DATABASE_TYPE=sqlite
BACKEND_PORT=8000
FRONTEND_PORT=8002

# Optimisations Performance
PYTHON_OPTIMIZE=2
NODE_ENV=production
PYTHONDONTWRITEBYTECODE=1
PYTHONUNBUFFERED=1

# Cache et Mémoire
ENABLE_CACHE=true
MAX_MEMORY_USAGE=2GB
ENABLE_COMPRESSION=true

# Sécurité Production
SECURE_MODE=true
DISABLE_DEBUG=true
ENABLE_LOGGING=true
